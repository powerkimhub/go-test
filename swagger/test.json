                    },
                    "400": {
                        "description": "Bad Request, possibly due to invalid JSON structure or missing fields",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "404": {
                        "description": "Resource Not Found",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    }
                }
            }
        }
