        "/keypair": {
            "post": {
                "description": "Generate a new KeyPair in the specified cloud connection.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "[CCM] KeyPair management"
                ],
                "summary": "Create a new KeyPair",
                "parameters": [
                    {
                        "description": "Request body for creating a KeyPair",
                        "name": "keyCreateReq",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.KeyCreateReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Details of the created KeyPair",
                        "schema": {
                            "$ref": "#/definitions/github_com_cloud-barista_cb-spider_cloud-control-manager_cloud-driver_interfaces_resources.KeyPairInfo"
                        }
                    },
                    "400": {
                        "description": "Bad Request, possibly due to invalid JSON structure or missing fields",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "404": {
                        "description": "Resource Not Found",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    }
                }
            }
        },
        "/vpc": {
            "get": {
                "description": "Retrieve a list of Virtual Private Clouds (VPCs) associated with a specific connection.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "[CCM] VPC management"
                ],
                "summary": "List VPCs",
                "parameters": [
                    {
                        "type": "string",
                        "description": "The name of the connection to list VPCs for",
                        "name": "connectionName",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of VPCs",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.ListVPCResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request, possibly due to invalid query parameter",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "404": {
                        "description": "Resource Not Found",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.SimpleMsg"
                        }
                    }
                }
            },
            "post": {
                "description": "Create a new Virtual Private Cloud (VPC) with specified subnet configurations.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "[CCM] VPC management"
                ],
                "summary": "Create VPC",
                "operationId": "create-vpc",
                "parameters": [
                    {
                        "description": "Request body for creating a VPC",
                        "name": "vpcCreateReq",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api-runtime_rest-runtime.vpcCreateReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Details of the created VPC",
                        "schema": {
                            "$ref": "#/definitions/github_com_cloud-barista_cb-spider_cloud-control-manager_cloud-driver_interfaces_resources.VPCInfo"
